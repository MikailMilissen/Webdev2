<%- include ('partials/header') %>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12 bg-primary border p-5">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title"><%=last.name%></h4>
                    <p class="card-text"><%=last.email%></p>
                    <p class="card-text">
                        <label>Address: </label>
                        <%=last.address.street%>, <%=last.address.suite%>
                        <%=last.address.city%>
                        <label>Phone: </label>
                        <%=last.phone%>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include ('partials/footer') %>